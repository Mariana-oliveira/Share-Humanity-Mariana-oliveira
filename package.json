{
  "name": "share-humanity-mariana-oliveira",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "nodemon -e scss -x \"npm run compiledev:sass\"",
    "compiledev:sass": "node-sass assets/sass/main.scss assets/css/main.css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",

    "compile:sass": "node-sass assets/sass/main.scss assets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" assets/css/style.comp.css -o assets/css/style.prefix.css",
    "compress:css": "node-sass assets/css/style.prefix.css assets/css/main.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mobwiser-IT-Academy/Share-Humanity-Mariana-oliveira.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mobwiser-IT-Academy/Share-Humanity-Mariana-oliveira/issues"
  },
  "homepage": "https://github.com/Mobwiser-IT-Academy/Share-Humanity-Mariana-oliveira#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
